<!-- Material form login -->
<div class="card">

  <h5 class="card-header info-color white-text text-center py-4">
    <strong>Sign in</strong>
  </h5>

  <!--Card content-->
  <div class="card-body px-lg-5 pt-0">

    <!-- Form -->
    <form action="/login/login" method="post">

      <!-- Email -->
      <div class="md-form">
        <input type="text" name="username"  id="materialLoginFormName" class="form-control">
        <label for="materialLoginFormName">Username</label>
      </div>

      <!-- Password -->
      <div class="md-form">
        <input type="password" name="password"  id="materialLoginFormPassword" class="form-control">
        <label for="materialLoginFormPassword">Password</label>
      </div>

     
      </div>
      <div class="form-group"> 

      <!-- Sign in button -->
      <button type="submit" class="btn btn-info btn-block my-1 waves-effect z-depth-0" >Sign in</button>
    </div>
     </form>

      
<div class="form-group"> <a href="/" id="btn" class="btn btn-danger btn-block my-1 waves-effect z-depth-0">HOME </a></div>
     

   
    <!-- Form -->

  </div>

</div>
<!-- Material form login -->
